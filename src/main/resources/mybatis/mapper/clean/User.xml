<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="bbackjk.test.caat.clean.user.adapter.out.persistence.dao.UserDao">

	<select id="findById"
            resultType="bbackjk.test.caat.clean.user.adapter.out.persistence.entity.UserEntity"
        >
        select *
        from user u
        where u.id = #{id}
    </select>
</mapper>
